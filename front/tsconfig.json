{
  "version": "1.5.0-alpha",
  "sortOutput": true,
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true
  },
  "filesGlob": [
    "./**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
